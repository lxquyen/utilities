<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SudokuBoardView">
        <attr name="lineColorPrimary" format="color" />
        <attr name="lineColorSecondary" format="color" />

        <attr name="lineStrokePrimary" format="dimension" />
        <attr name="lineStrokeSecondary" format="dimension" />

        <attr name="boardColor" format="color" />
        <attr name="backgroundSelectedColor" format="color" />

        <attr name="textCellColorPrimary" format="color" />
        <attr name="textCellColorSecondary" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="cpv_progress" format="float" />
        <attr name="cpv_maxProgress" format="float" />
        <attr name="cpv_animDuration" format="integer" />
        <attr name="cpv_animSwoopDuration" format="integer" />
        <attr name="cpv_animSyncDuration" format="integer" />
        <attr name="cpv_color" format="color" />
        <attr name="cpv_thickness" format="dimension" />
        <attr name="cpv_indeterminate" format="boolean" />
        <attr name="cpv_animAutoStart" format="boolean" />
        <attr name="cpv_animSteps" format="integer" />
        <attr name="cpv_startAngle" format="float" />
    </declare-styleable>

    <declare-styleable name="TwoCircularProgressView">
        <attr name="tcpv_color_start" format="color" />
        <attr name="tcpv_color_end" format="color" />
        <attr name="tcpv_thickness" format="dimension" />
        <attr name="tcpv_space" format="dimension" />
    </declare-styleable>
</resources>